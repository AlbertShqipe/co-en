<%- if controller_name != 'sessions' %>
  <%= link_to t("session_links.link1"), new_session_path(resource_name), class:"session-link" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to t("session_links.link2"), new_registration_path(resource_name), class:"session-link" %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to t("session_links.link3"), new_password_path(resource_name), class:"session-link" %><br />
<% end %>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to t("session_links.link4"), new_confirmation_path(resource_name), class:"session-link" %><br />
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to t("session_links.link5"), new_unlock_path(resource_name), class:"session-link" %><br />
<% end %>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= button_to "`#{t("session_links.link6")}` #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), data: { turbo: false }, class:"session-link" %><br />
  <% end %>
<% end %>
